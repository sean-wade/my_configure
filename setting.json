    // "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Allman, SpaceAfterTemplateKeyword: true, SpaceBeforeAssignmentOperators: true, SpacesBeforeTrailingComments: 1, AlignConsecutiveMacros: true, AlignConsecutiveDeclarations: true, BinPackParameters: false, AlignAfterOpenBracket: AlwaysBreak, AlwaysBreakTemplateDeclarations: true, AfterEnum: true, AfterFunction: true, AfterStruct: true}",

}
